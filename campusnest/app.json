{
  "expo": {
    "name": "campusnest",
    "slug": "campusnest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/landing_page_bottom_logo.png",
    "scheme": "campusnest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
